//HARSHAD NUMBER
#include<iostream>
using namespace std;
int main()
{
    int n,r,sum=0;
    cin>>n;
    int temp=n;
    while(n!=0)
    {
        r=n%10;
        sum+=r;
        n/=10;
    }
  if(temp%sum==0)
  {
    cout<<"True";
  }
  else
  {
    cout<<"False";
  }
}




//FIBONACCI
import java.io.*;
class Input //FIBONACCI SERIES
{
	public static void main(String args[])throws Exception
	{
		InputStreamReader isr=new InputStreamReader(System.in);
		BufferedReader br=new BufferedReader(isr);
		int n;
		n=Integer.parseInt(br.readLine());
		int f=0,p;
		int s=1;
		System.out.print(f+" "+s+" ");
		for(int i=3;i<n;i++)
		{
			p=f+s;
			f=s;
			s=p;
		  System.out.print(p+" ");
		}
		System.out.println(f+s);
	}
}


